    "element-plus": "^2.13.1",
    "nuxt": "^4.2.2",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/test-utils": "^3.23.0",
    "@vitest/coverage-v8": "^4.0.17",
